<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <KeepRunningUntilFailure>
            <ForceSuccess>
                <Sequence>
                    <Navegar2TR>
                    <DataCheckTR>
                    <Fallback>
                        <ControlObservadorTR>
                        <Sequence>
                            <Navegar3TR>
                            <ObservarTR>
                        </Sequence>
                    </Fallback>
                    <NavegarTR>
                    <DataDumpTR>
                </Sequence>
            </ForceSuccess>
        </KeepRunningUntilFailure>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
       <Action ID="ObservarTR"/>
       <Action ID="DataCheckTR"/>
       <Action ID="DataDumpTR"/>
       <Action ID="Navegar3TR"/>
       <Action ID="NavegarTR"/>
       <Action ID="Navegar2TR"/>
       <Condition ID="ControlObservadorTR"/>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

